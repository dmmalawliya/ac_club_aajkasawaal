//find element in a sorted array return 1 if found,return -1 if not found
class Solution{
    public:
    // Function to find element in sorted array
    // arr: input array
    // N: size of array
    // K: element to be searche
    int searchInSorted(int arr[], int N, int K) 
    { 
        int i;
       // Your code here
       for(i=0;i<N;i++)
       {    if(K==arr[i])
           { return 1;
           }
           
       }
       return -1;
    }
};
time complexiety O(n)
//print missing number

int missingNumber(int A[], int N)
{
    // Your code goes here
    int i,j,s,sum=0;
    s=N*(N+1)/2;
    for(i=0;i<N-1;i++)
    {   sum+=A[i];
    }
    return s-sum;
    
}
time complexiety O(n)
